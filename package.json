{
  "name": "trading-tools",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "eslint-print-config": "eslint --print-config src/index.tsx > misc/eslint-print-config.json",
    "lint-dry": "npm-run-all --parallel --continue-on-error lint:eslint-dry lint:tsc",
    "lint:eslint-dry": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src",
    "lint:eslint": "eslint -c .eslintrc.js --fix --ext .ts,.tsx,.js src",
    "lint:tsc": "tsc --pretty --noEmit",
    "lint": "npm-run-all --parallel --continue-on-error lint:eslint lint:tsc",
    "test-ci": "jest --ci",
    "test": "jest --watch",
    "update-all": "npm-check-updates -u"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@testing-library/react": "10.2.1",
    "@types/jest": "26.0.0",
    "@types/node": "14.0.13",
    "@types/react": "16.9.36",
    "@types/testing-library__react": "10.0.1",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "babel-jest": "26.0.1",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-flowtype": "5.1.3",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-typescript-sort-keys": "1.2.0",
    "import-sort-style-module": "6.0.0",
    "jest": "26.0.1",
    "jest-watch-typeahead": "0.6.0",
    "next": "9.4.4",
    "next-pwa": "2.6.0",
    "npm-check-updates": "7.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prettier-plugin-import-sort": "0.0.4",
    "typescript": "3.9.5"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
